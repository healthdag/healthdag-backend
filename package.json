{
    "name": "healthlease",
    "version": "1.0.0",
    "description": "HealthLease application built with Hono",
    "type": "module",
    "main": "src/app.ts",
    "scripts": {
        "dev": "bun run --hot src/app.ts",
        "start": "bun run src/app.ts",
        "build": "bun build src/app.ts --outdir dist --target bun",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "hono",
        "typescript",
        "api",
        "health"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "hono": "^4.6.3",
        "@hono/node-server": "^1.12.0"
    },
    "devDependencies": {
        "@types/node": "^22.10.2",
        "typescript": "^5.7.2",
        "bun-types": "latest"
    },
    "engines": {
        "bun": ">=1.0.0"
    }
}